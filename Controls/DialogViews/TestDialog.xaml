<?xml version="1.0" encoding="utf-8" ?>
<ContentView x:Class="Controls.DialogViews.TestDialog"
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:dialogs="using:Prism.Dialogs"
             Width="150"
             BackgroundColor="Red"
             HeightRequest="300">
    <VerticalStackLayout>
        <Label HorizontalOptions="Center"
               Text="Welcome to .NET MAUI!"
               VerticalOptions="Center" />
        <Button Command="{Binding CloseDialogCommand}" Text="CloseDialog">
            <!--<Button.CommandParameter>
                <x:Static Member="dialogs:ButtonResult.OK" />
            </Button.CommandParameter>-->
        </Button>
    </VerticalStackLayout>
</ContentView>
